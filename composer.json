{
  "name": "kanti/letsencrypt-client",
  "license": "MIT",
  "config": {
    "platform": {
      "php": "8.0"
    },
    "sort-packages": true,
    "optimize-autoloader": true,
    "process-timeout": 0,
    "allow-plugins": {
      "phpro/grumphp": true,
      "phpstan/extension-installer": true,
      "pluswerk/grumphp-config": true
    }
  },
  "scripts": {
    "test": "./vendor/bin/phpunit tests",
    "test:watch": "./vendor/bin/phpunit-watcher watch tests",
    "post-update-cmd": [
      "composer bump"
    ]
  },
  "require": {
    "php": "8.0.*",
    "ext-curl": "*",
    "ext-yaml": "*",
    "symfony/config": "^5.4.21",
    "symfony/console": "^5.4.22",
    "symfony/dependency-injection": "^5.4.22",
    "symfony/error-handler": "^5.4.21",
    "symfony/finder": "^5.4.21",
    "symfony/process": "^5.4.22",
    "symfony/var-dumper": "^5.4.22",
    "symfony/yaml": "^5.4.21",
    "thecodingmachine/safe": "^2.5"
  },
  "autoload": {
    "psr-4": {
      "Kanti\\LetsencryptClient\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Kanti\\LetsencryptClient\\Tests\\": "tests"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "*",
    "pluswerk/grumphp-config": "^6.4.1",
    "spatie/phpunit-watcher": "*"
  },
  "type": "project"
}
